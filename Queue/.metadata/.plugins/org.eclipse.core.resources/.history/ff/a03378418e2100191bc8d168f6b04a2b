package com.sai.programs;

public class Fibonacci {

	public static void main(String[] args) {
		
		fibo(5);
		
		

	}

	public static void fibo(int val) {
		int prev1 = 0;
		int prev2 =1;
		int sum;
		 for(int i = 0 ;i<5;i++) {
			 sum = prev1+prev2;
			 System.out.println(sum);
			 prev1=prev2;
			 prev2 = sum;
		 }
	}



}
