package com.queue.implementation;

public class LinkedListStack {
	
	public Node top;
	public int size;
	public Node dis;
	
	public LinkedListStack() {
		this.top = null;
	}
	
	public void push( int newValue) {
		
		Node temp = new Node(newValue);
		
		temp.next=top;
		
		
	}
	
	public void display() {
		
		Node temp =this.dis;
		System.out.println();
		while(temp!=null) {
			System.out.print(top.value);
			temp=temp.next;
		}
		
		System.out.print("<== top");
	}
	
	public Node pop() {
		
		if(this.top!=null) {
			
		}
		
		
		return null;
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
